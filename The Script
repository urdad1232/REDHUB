local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()

local Window = Library.CreateLib("RedHub V1", "BloodTheme")

local Tab = Window:NewTab("Scripts")

local ScriptsSection = Tab:NewSection("Da hood")

ScriptsSection:NewButton("Faded", "Da hood script", function()
    _G.Toggles = "V" -- You can put any keybind
loadstring(game:HttpGet("https://raw.githubusercontent.com/NighterEpic/Faded/main/YesEpic", true))()
end)

ScriptsSection:NewButton("Vortex", "Da hood script", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/ImagineProUser/vortexdahood/main/vortex", true))()
end)

local ScriptsSection = Tab:NewSection("Admin")

ScriptsSection:NewButton("Cmd-X", "FE Admin", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/CMD-X/CMD-X/master/Source", true))()
end)

ScriptsSection:NewButton("Infinite Yield", "FE Admin", function()
    loadstring(game:HttpGet(('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'),true))()
end)

local ScriptsSection = Tab:NewSection("Jailbreak")

ScriptsSection:NewButton("SubHub", "Jailbreak GUI", function()
    pcall(function()
  loadstring(game:HttpGet("https://raw.githubusercontent.com/Kw6m/Subbers-scripts/main/jailbreak", true))()
end)
end)

